\n      this.city = city;\n      this.fullname = () => {\n        return this.fname + \" \" + this.lname + \", \" + this.city;\n      }\n    }\n\n    // Lists containing names and cities for random generation\n    let firstList = [\"John\", \"Mary\", \"Abe\", \"Jim\", \"Stanley\", \"Salley\", \"Ann\"];\n    let lastList = [\"Rodes\", \"Smith\", \"Miller\", \"Jones\", \"Stucci\", \"Kindreck\", \"Patton\"];\n    let cityList = [\"Kali\", \"Polson\", \"Libby\", \"Whitefish\", \"Dayton\", \"Noxon\", \"Bozeman\"];\n\n    let people = new Array(); // Array to store Person objects\n\n    // Function to generate a random integer between 0 (inclusive) and max (inclusive)\n    function RandomIntMax(max) {\n      // + 1 to make the max inclusive\n      return Math.floor(Math.random() * (max + 1));\n    }\n\n    function createPeople() {\n\n      people.length = 0;\n\n      let numPeople = Number(document.getElementById(\"numppl\").value);\n\n      if (numPeople > 25) {\n        numPeople = 25;\n      }\n\n      for (let i = 0; i < numPeople; i++) {\n        const rfname = firstList[RandomIntMax(firstList.length - 1)];\n        const rlname = lastList[RandomIntMax(lastList.length - 1)];\n        const rcity = cityList[RandomIntMax(cityList.length - 1)];\n\n        const xperson = new Person(rfname, rlname, rcity); // Use `new` keyword to create a new Person object\n        people[i] = xperson;\n      }\n\n      document.getElementById(\"numppl\").value = '';\n      displayPeople();\n    }\n\n\n    // Function to display the full names and cities of all Person objects in the people array\n    function displayPeople() {\n      let strpeople = \"\";\n\n      for (let i = 0; i < people.length; i++) {\n        strpeople += people[i].fullname() + \"\\n<br>\"; // Use the fullname method of each Person object\n      }\n      console.log(strpeople);\n      document.getElementById(\"ppl\").innerHTML = strpeople;\n    }\n  </script>\n</head>\n\n<body>\n  <div class=\"theContent\">\n    <h1>Objects By Function</h1>\n\n    <br><hr><br>\n\n    <label for=\"numppl\">Enter the number of people:</label>\n    <input type=\"number\" name=\"numppl\" id=\"numppl\" min=\"0\" max=\"25\" value=\"0\" placeholder=\"1-25\">\n    <button onclick=\"createPeople()\">Enter</button>\n\n    <p id=\"ppl\"></p>\n\n    <p><a href=\"index.html\">Objects By Class</a></p>\n  </div>\n</body>\n\n</html>\n","position":{"start":{"line":9,"column":1},"end":{"line":107,"column":1}},"key":"Srzesp7Ila"},{"type":"heading","depth":2,"position":{"start":{"line":111,"column":1},"end":{"line":111,"column":1}},"children":[{"type":"text","value":"Explaination","position":{"start":{"line":111,"column":1},"end":{"line":111,"column":1}},"key":"NI09KLDt94"}],"identifier":"explaination","label":"Explaination","html_id":"explaination","implicit":true,"key":"uF2EFu3iyo"},{"type":"paragraph","position":{"start":{"line":113,"column":1},"end":{"line":113,"column":1}},"children":[{"type":"text","value":"This code creates a webpage that allows users to generate a random list of people with full names and cities. Here’s a breakdown of the different parts:","position":{"start":{"line":113,"column":1},"end":{"line":113,"column":1}},"key":"FrSYA2TyEs"}],"key":"C7dkrrRnrJ"},{"type":"paragraph","position":{"start":{"line":115,"column":1},"end":{"line":115,"column":1}},"children":[{"type":"strong","position":{"start":{"line":115,"column":1},"end":{"line":115,"column":1}},"children":[{"type":"text","value":"HTML (Structure and Appearance):","position":{"start":{"line":115,"column":1},"end":{"line":115,"column":1}},"key":"m4W6h2xDT2"}],"key":"AkUkZuRDO0"}],"key":"FZbWjw8jcx"},{"type":"list","ordered":false,"spread":false,"position":{"start":{"line":117,"column":1},"end":{"line":140,"column":1}},"children":[{"type":"listItem","spread":true,"position":{"start":{"line":117,"column":1},"end":{"line":118,"column":1}},"children":[{"type":"paragraph","position":{"start":{"line":117,"column":1},"end":{"line":117,"column