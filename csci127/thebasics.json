-----------------------\n#   containers - discuss more in future - just show\n# ---------------------------------------\n\nprint(\"\\n*** Lists ***\\n\")      # Explain \\n and \\t\n\n### LISTS ###\n## properties: ordered, iterable, mutable, can contain multiple data types\nmylista = [1,2,3,4]\nprint(mylista)\n\nmylistb = [\"larry\", \"curly\", \"moe\"]\nprint(mylistb)\n\nmylistc = [42, 42.0, \"forty two\", True, ['Tom', 'Dick', 'Harry']]\nprint(mylistc)\n\n### TUPLES ###\n## properties: ordered, iterable, immutable, can contain multiple data types\n## like lists, but they don't change size\nmytuplea = (1,2,3)\nprint(mytuplea)\n\nmytupleb = (\"Ducks\", \"Goats\", \"Cows\")\nprint(mytupleb)\n\nmytuplec = (1, 1.1, \"one\", False, [42, 43, 44, 45])\nprint(mytuplec)\nprint(type(mytuplec))\n\n\n### DICTIONARIES ###\n## properties: unordered, iterable, mutable, can contain multiple data types\n## made of key-value pairs\n## keys must be unique, and can be strings, numbers, or tuples\n## values can be any type\n\n# ---------------------------------------\n\nprint(\"\\n*** Dictionaries ***\\n\")\n\n### DICTIONARIES ###\n## properties: unordered, iterable, mutable, can contain multiple data types\n## made of key-value pairs\n## keys must be unique, and can be strings, numbers, or tuples\n## values can be any type\n\nmydict = {\"key1\": \"value1\", \"key2\": \"value2\", \"key3\": \"value3\" }\nprint(mydict)\n\n# student number, last name\nmydict1 = {111 : \"Smith\", 222 : \"Doe\", 333 : \"Baker\"}\nprint(mydict1)\nprint(type(mydict1))\n\n#show error\nmydict1 = {111 : \"Smith\", 222 : \"Doe\", 333 : \"Baker\", 111 : \"Adam\"}\n\n# ---------------------------------------\n\nprint(\"\\n*** Sets ***\\n\")\n\n### SETS ###\n## properties: unordered, iterable, mutable, can contain multiple data types\n## made of unique elements (strings, numbers, or tuples)\n## like dictionaries, but with keys only (no values)\n\n#like dictionaries, but leave out the values\nmyset1 = {'python', 'r', 'java', 'c#'} \nprint(myset1)\n\nmyset2 = {1, 2, 3, 4, 5, 6}\nprint(myset2)\nprint(type(myset2))\n\n#show error\nmyset3 = {11,22,33,33, 44,55}\nprint(myset3)\n\n# -------------------------------------","position":{"start":{"line":85,"column":1},"end":{"line":197,"column":1}},"key":"EkiEfetRPC"},{"type":"paragraph","position":{"start":{"line":201,"column":1},"end":{"line":201,"column":1}},"children":[{"type":"strong","position":{"start":{"line":201,"column":1},"end":{"line":201,"column":1}},"children":[{"type":"text","value":"Example Showing Mathematical Operations","position":{"start":{"line":201,"column":1},"end":{"line":201,"column":1}},"key":"ePOQdtQ3U5"}],"key":"yZbzbvXu0e"}],"key":"jdQ9e2XNuY"},{"type":"code","lang":"python","value":"print(\"Get and display input \" )\ndogname = input(\"Enter a dog name \")\ndogage = float(input(\"Enter the dog age in years  \"))\n\n# numbers must be in a str() function\nprint(\"Dog name = \" +  dogname + \"  age = \" + str(dogage))\nprint(\"\\n\\n\")\n\n\n\nprint(\"-- operations review --\")\nprint(\"Parenthesis 2 + 5 * 7 vs (2 + 5) * 7\")\nx = 2 + 5 * 7\nprint(x)\nx = (2 + 5) * 7\nprint(x)\n\nprint(\"3 to the power of 3 3**3\")\nx = 3**3\nprint(x)\n\na = 10.5\nb = 2.5\n\n\nprint(\"Add and subtract 10.5 2.5\")\nx = a + b\nprint(x)\n\nx = a - b\nprint(x)\n\nprint(\"Multiple and divide 10.5 2.5\")\n\nx = a * b\nprint(x)","position":{"start":{"line":203,"column":1},"end":{"line":240,"column":1}},"key":"nrRXC2rFdW"},{"type":"thematicBreak","position":{"start":{"line":242,"column":1},"end":{"line":242,"column":1}},"key":"unQkLHZkBO"},{"type":"paragraph","position":{"start":{"line":244,"column":1},"end":{"line":244,"column":1}},"children":[{"type":"text","value":"End Of Topic","position":{"start":{"line":244,"column":1},"end":{"line":244,"column":1}},"key":"wW6exVue1m"}],"key":"qrXPD4VnqV"}],"key":"zp1jyjBpmy"}],"key":"zvtal4rTxQ"},"references":{"cite":{"order":[],"data":{}}},"footer":{"navigation":{"prev":{"title":"Readings","url"